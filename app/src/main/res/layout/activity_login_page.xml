<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".login_page">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFF2D8"
        tools:layout_editor_absoluteX="2dp"
        tools:ignore="MissingConstraints">


        <ImageView
            android:id="@+id/logoApps3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="147dp"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="146dp"
            android:src="@drawable/logoaps" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="291dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="53dp"
            android:layout_marginTop="112dp"
            android:layout_marginEnd="52dp"
            android:text="Sign in"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="30dp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editTextText"
            android:layout_width="296dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="48dp"
            android:textColor="@color/black"
            android:layout_marginTop="199dp"
            android:layout_marginEnd="49dp"
            android:background="@drawable/cstm_edtt"
            android:drawableStart="@drawable/baseline_account_box_24"
            android:ems="10"
            android:hint="   Email"

            android:textColorHint="@color/black"
            android:inputType="text"
            android:paddingStart="10dp" />

        <EditText
            android:id="@+id/editTextTextPassword"
            android:layout_width="296dp"
            android:layout_height="46dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="275dp"
            android:layout_marginEnd="48dp"
            android:background="@drawable/cstm_edtt"
            android:drawableStart="@drawable/password_svgrepo_com__1_"
            android:ems="10"
            android:hint="   Password"
            android:textColor="@color/black"
            android:inputType="textPassword"
            android:textColorHint="@color/black"
            android:paddingStart="10dp" />

        <Button
            android:id="@+id/tologin2"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="427dp"
            android:layout_marginEnd="50dp"
            android:backgroundTint="#113946"
            android:text="Log in"
            android:textSize="17dp"
            android:textColor="#EAD7BB"
            app:cornerRadius="15dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="291dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="53dp"
            android:layout_marginTop="515dp"
            android:layout_marginEnd="51dp"
            android:text="Doesn't have an Account?"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="15dp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/toSignup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="142dp"
            android:layout_marginTop="537dp"
            android:layout_marginEnd="140dp"
            android:backgroundTint="#FEF1D7"
            android:backgroundTintMode="src_atop"
            android:text="Sign Up"
            android:textColor="#2B2926"
            app:cornerRadius="15dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="216dp"
            android:layout_marginTop="359dp"
            android:layout_marginEnd="142dp"
            android:src="@drawable/icons8_google" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="149dp"
            android:layout_marginTop="358dp"
            android:layout_marginEnd="209dp"
            android:src="@drawable/_296520_bubble_chat_mobile_whatsapp_whatsapp_logo_icon" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="286dp"
            android:layout_marginTop="357dp"
            android:layout_marginEnd="72dp"
            android:src="@drawable/tiktok_square_color_icon" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="68dp"
            android:layout_marginTop="360dp"
            android:layout_marginEnd="267dp"
            android:src="@drawable/_296499_fb_facebook_facebook_logo_icon" />


    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>