<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".signup_page">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFF2D8"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="2dp">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="281dp"
            android:layout_marginTop="532dp"
            android:layout_marginEnd="74dp"
            android:src="@drawable/tiktok_square_color_icon" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="141dp"
            android:layout_marginTop="532dp"
            android:layout_marginEnd="212dp"
            android:src="@drawable/_296520_bubble_chat_mobile_whatsapp_whatsapp_logo_icon" />

        <Button
            android:id="@+id/btnRegis"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="51dp"
            android:layout_marginTop="612dp"
            android:layout_marginEnd="50dp"
            android:backgroundTint="#113946"
            android:text="Sign Up"
            android:textColor="#EAD7BB"
            android:textSize="17dp"
            app:cornerRadius="15dp" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="211dp"
            android:layout_marginTop="532dp"
            android:layout_marginEnd="145dp"
            android:src="@drawable/icons8_google" />

        <TextView
            android:id="@+id/tvstatus"
            android:layout_width="124dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="135dp"
            android:layout_marginTop="691dp"
            android:layout_marginEnd="134dp"
            android:layout_marginBottom="80dp"
            android:text="TextView"
            android:textAlignment="center"
            android:textSize="15dp" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="61dp"
            android:layout_marginTop="532dp"
            android:layout_marginEnd="270dp"
            android:src="@drawable/_296499_fb_facebook_facebook_logo_icon" />

        <EditText
            android:id="@+id/inputEmail"
            android:layout_width="296dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="270dp"
            android:layout_marginEnd="50dp"
            android:background="@drawable/cstm_edtt"
            android:drawableStart="@drawable/email"
            android:ems="10"
            android:textColor="@color/black"
            android:hint="   Email"
            android:inputType="text"
            android:paddingStart="10dp"
            android:textColorHint="@color/black" />

        <EditText
            android:id="@+id/inputUsername"
            android:layout_width="296dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="210dp"
            android:textColor="@color/black"
            android:layout_marginEnd="50dp"

            android:background="@drawable/cstm_edtt"
            android:drawableStart="@drawable/baseline_account_box_24"
            android:ems="10"
            android:hint="   Username"
            android:inputType="text"
            android:paddingStart="10dp"
            android:textColorHint="@color/black" />

        <EditText
            android:id="@+id/inputPhone"
            android:layout_width="296dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="330dp"
            android:layout_marginEnd="50dp"
            android:background="@drawable/cstm_edtt"
            android:drawableStart="@drawable/baseline_phone_enabled_24"
            android:ems="10"
            android:textColor="@color/black"
            android:hint="   Phone"
            android:inputType="phone"
            android:paddingStart="10dp"
            android:textColorHint="@color/black" />

        <EditText
            android:id="@+id/inputPassword"
            android:layout_width="296dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="390dp"
            android:layout_marginEnd="50dp"
            android:background="@drawable/cstm_edtt"
            android:drawableStart="@drawable/password_svgrepo_com__1_"
            android:ems="10"
            android:textColor="@color/black"
            android:hint="   Password"
            android:textColorHighlight="@color/black"
            android:inputType="textPassword"
            android:paddingStart="10dp"
            android:textColorHint="@color/black" />

        <EditText
            android:id="@+id/inputRepass"
            android:layout_width="296dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="50dp"
            android:layout_marginTop="456dp"
            android:layout_marginEnd="51dp"
            android:background="@drawable/cstm_edtt"
            android:drawableStart="@drawable/password_svgrepo_com__1_"
            android:ems="10"
            android:hint="   Re-Password"
            android:inputType="textPassword"
            android:paddingStart="10dp"
            android:textColor="@color/black"
            android:textColorHighlight="@color/black"
            android:textColorHint="@color/black" />

        <ImageView
            android:id="@+id/logoApps4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="147dp"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="146dp"
            android:src="@drawable/logoaps" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="291dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="53dp"
            android:layout_marginTop="125dp"
            android:layout_marginEnd="52dp"
            android:text="Sign Up"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="30dp"
            android:textStyle="bold" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>